#include <stdio.h>

int main() {
    int base;
    int result = 1;

    printf("Please enter a base:\n");
    scanf("%d", &base);

    printf("The powers of %d are:\n\n", base);

    for (int i = 0; i <= 8; i++) {
        printf("%d ^ %d is %d\n", base, i, result);
        result *= base;
    }

    return 0;
}
